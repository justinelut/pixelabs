{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "preserve",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/checkout/index.ts", "collections/Category.js", "components/client/project.js", "access/isAdmin.ts", "globals/Footer.ts"  ],
  "exclude": [
    "node_modules"
  ]
}
